{
  "liveServer.settings.donotShowInfoMsg": true,
  "liveServer.settings.donotVerifyTags": true,
  "liveServer.settings.useLocalIp": true,
  "diffEditor.wordWrap": "off",
  "editor.wordWrap": "on",
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "highlight-matching-tag.styles": {
    "opening": {
      "left": {
        "custom": {
          "borderWidth": "0 0 0 3px",
          "borderStyle": "solid",
          "borderColor": "yellow",
          "borderRadius": "5px",
          "overviewRulerColor": "white"
        }
      },
      "right": {
        "custom": {
          "borderWidth": "0 3px 0 0",
          "borderStyle": "solid",
          "borderColor": "yellow",
          "borderRadius": "5px",
          "overviewRulerColor": "white"
        }
      }
    }
  },

  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "editor.suggestSelection": "first",
  "editor.hover.above": true,
  "editor.autoClosingBrackets": "always",
  "editor.codeActionsOnSave": { "source.organizeImports": true },
  "editor.guides.bracketPairs": true,
  "js/ts.implicitProjectConfig.checkJs": true,
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  // "files.associations": { "*.js": "javascriptreact" },

  "emmet.includeLanguages": {
    "csharp": "html",
    "html.autoClosingTags": "html",
    "html": "html",
    "javascript": "javascriptreact",
    "vue-html": "html",
    "razor": "html",
    "plaintext": "jade",
    "scss": "css"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "emmet.showExpandedAbbreviation": "always",
  "liveServer.settings.CustomBrowser": "firefox",
  "prettier.enableDebugLogs": true,
  "prettier.singleQuote": true,
  "tabnine.experimentalAutoImports": true,
  "git.enableSmartCommit": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css"
      // "savePath": "/css"
    }
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "liveSassCompile.settings.excludeList": ["**/node_modules/**", ".vscode/**"],
  "liveSassCompile.settings.includeItems": [],
  "vsintellicode.features.python.deepLearning": "enabled",
  "pythonIndent.useTabOnHangingIndent": true,
  "javascript.format.enable": true,
  "python.autoComplete.addBrackets": true,
  "kite.showWelcomeNotificationOnStartup": false,
  "code-runner.saveAllFilesBeforeRun": true,
  "code-runner.saveFileBeforeRun": true,
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd src/ && javac $fileName && java $fileNameWithoutExt  && cd ..",
    "c": "gcc -o $fileNameWithoutExt $fileName && ./$fileNameWithoutExt ",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "python -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "dotnet run $fileName",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runhaskell",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt"
  },
  "liveServer.settings.fullReload": true,
  "python.linting.pylintArgs": ["--generate-members"],
  "liveServer.settings.wait": 10,
  "vim.commandLineModeKeyBindings": [],
  "vim.insertModeKeyBindingsNonRecursive": [],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<Esc>"],
      "after": ["n"]
    }
  ],
  "vim.insertModeKeyBindings": [],
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.wrapTabs": true,
  "workbench.iconTheme": "vscode-icons",

  "files.defaultLanguage": "${activeEditorLanguage}",
  "cSpell.userWords": [
    "Calvyn",
    "Hllxdzcaa",
    "Siong",
    "browserslist",
    "checkfor",
    "jsxdev"
  ],
  "debug.javascript.terminalOptions": {},
  "tailwindCSS.emmetCompletions": true,
  "html-css-class-completion.enableEmmetSupport": true,
  "prettier.jsxSingleQuote": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "css.validate": false,
  "quokka.colors": {
    "covered": "#62b455",
    "errorPath": "#ffa0a0",
    "errorSource": "#fe536a",
    "notCovered": "#cccccc",
    "partiallyCovered": "#d2a032"
  },
  "discord.lowerDetailsNoWorkspaceFound": "Nothing to see here",
  "emmet.excludeLanguages": [],
  "python.analysis.completeFunctionParens": true,
  "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "terminal.integrated.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
  "markdown.preview.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
  "editor.fontSize": 16,
  "material-ui-snippets.showNotesOnStartup": false,
  "gitlens.advanced.messages": {
    "suppressImproperWorkspaceCasingWarning": true
  },
  "terminal.integrated.tabs.enabled": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 3,
  "search.seedWithNearestWord": true,
  "terminal.integrated.cursorStyle": "line",
  "pdf-preview.default.cursor": "hand",
  "prettier.eslintIntegration": true,
  "prettier.bracketSpacing": true,
  "files.autoSaveDelay": 100,
  "editor.formatOnSave": true,
  "path-intellisense.autoSlashAfterDirectory": true,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "editor.quickSuggestions": true,
  "terminal.integrated.defaultProfile.windows": "Ubuntu-20.04 (WSL)",
  "terminal.integrated.automationShell.windows": "C:\\Program Files\\Git\\git-bash.exe",
  "terminal.integrated.copyOnSelection": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.tabSize": 2,
  "javascript.suggest.names": false,
  "prettier.tabWidth": 2,
  "prettier.useTabs": false,
  "better-comments.highlightPlainText": true,
